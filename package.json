{
  "name": "Tampa-Underground-Coaching",
  "version": "0.1.0",
  "description": "PWA for empowering Underground Coaching Department",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "node server.js",
    "build": "node build/build.js",
    "rebuild-prod": "NODE_ENV=production git clean -dfx && npm install --production",
    "copy-fonts": "cpy node_modules/framework7-icons/fonts/*.* src/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} src/fonts",
    "postinstall": "npm run copy-fonts && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "ios",
    "webpack",
    "framework7",
    "vue",
    "vuejs",
    "material",
    "mobile",
    "app",
    "f7"
  ],
  "author": "Josh Loya",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/framework7io/framework7-template-vue-webpack/issues"
  },
  "homepage": "http://underground.coach/",
  "dependencies": {
    "auth0-js": "9.2.2",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.28.0",
    "eventemitter3": "3.0.1",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "framework7": "^2.0.7",
    "framework7-icons": "^0.8.9",
    "framework7-vue": "^2.0.7",
    "html-webpack-plugin": "^2.30.1",
    "material-design-icons": "^3.0.1",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "postcss-import": "^11.0.0",
